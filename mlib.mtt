<mlib bin="::haxelibOutput::">
	<resources>
	    ::foreach classPaths::	<resource type="src" path="::__current__::" dest="" />
	    ::end::
		::if licenseFile::<resource type="license" path="::licenseFile::" />::end::
		::if runFile::<resource type="run" path="::runFile::" />::end::
        ::foreach resources::	<resource  path="::path::" dest="::dest::" />
	    ::end::
	</resources>
</mlib>
